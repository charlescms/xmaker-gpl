  VclSmp50,
  Vclx50;

contains
  RxCtrls in 'RxCtrls.pas',
  RxGrids in 'RxGrids.pas',
  CurrEdit in 'CurrEdit.pas',
  ToolEdit in 'ToolEdit.pas',
  DateUtil in 'DateUtil.pas',
  RXSplit in 'RXSplit.pas',
  RxSlider in 'RxSlider.pas',
  RxClock in 'RxClock.pas',
  Animate in 'Animate.pas',
  RxSpin in 'RxSpin.pas',
  RxSwitch in 'RxSwitch.pas',
  RxDice in 'RxDice.pas',
  VCLUtils in 'VCLUtils.pas',
  AniFile in 'AniFile.pas',
  IcoList in 'IcoList.pas',
  ObjStr in 'ObjStr.pas',
  RxCombos in 'RxCombos.pas',
  PickDate in 'PickDate.pas',
  MaxMin in 'MaxMin.pas',
  RxConst in 'RxConst.pas',
  RxCConst in 'RxCConst.pas',
  AppUtils in 'AppUtils.pas',
  StrUtils in 'StrUtils.pas',
  FileUtil in 'FileUtil.pas',
